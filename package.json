{
  "name": "com.whpl.tracker",
  "displayName": "Tracker",
  "version": "1.0.0",
  "description": "A simple tracker application",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && rm -r www/* && cp -r dist/workout-tracker/browser/* www",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:workout-tracker": "node dist/workout-tracker/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
