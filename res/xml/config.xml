<?xml version='1.0' encoding='utf-8'?>
<widget id="com.whpl.tracker" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <name>Fitness Tracker</name>
    <description>A simple Fitness tracker application</description>
    <author email="careers@websiterahouse.com" href="https://websiterahouse.com">
        Websitera House Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="KeepRunning" value="true" />
    <preference name="InAppBrowserStorageEnabled" value="true" />
    <preference name="AndroidWindowSplashScreenBackground" value="#2D0D4F" />
</widget>
